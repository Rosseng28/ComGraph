#include<iostream>
using namespace std;

int main()
{
    int n, i, j; 
    cout << "Enter an Integer: "; 
        cin >> n; 
    cout << "Enter Range: ";
        cin >> j;
            cout << "\n";
        for (i = 1; i <=j; ++i){ 
                    cout << n << " * " << i << " = " << n * i << endl; 
                        }
    cout << "\n\nProgrammed by: Rosalinda Lebosada"; 
   return 0;
   }
